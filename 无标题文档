configtxgen -outputBlock ./channel-artfacts/genesis.block -profile TwoOrgsOrdererGenesis -channelID fabric-channel //生成创世区块


configtxgen -profile TwoOrgsChannel -outputCreateChannelTx ./channel-artfacts/channel.tx -channelID mychannel


configtxgen -profile TwoOrgsChannel -outputAnchorPeersUpdate ./channel-artifacts/Org1MSPanchors.tx -channelID mychannel -asOrg Org1MSP

configtxgen -profile TwoOrgsChannel -outputAnchorPeersUpdate ./channel-artifacts/Org2MSPanchors.tx -channelID mychannel -asOrg Org2MSP

cp /home/llh/gopath/src/github.com/hyperledger/fabric-samples/test-network/docker/docker-compose-test-net.yaml ./


